)ATTR
 # TYPE(TEXT) INTENS(LOW)  CAPS(OFF) COLOR(BLUE) SKIP(ON)
 ! TYPE(TEXT) INTENS(LOW)  CAPS(OFF) COLOR(WHITE)
 @ TYPE(INPUT)  INTENS(LOW)  CAPS(ON) COLOR(RED)
 + TYPE(TEXT)  INTENS(LOW)  CAPS(ON) COLOR(TURQ)
)BODY WINDOW(30,5)

!Specify address space
@Z!1.+Userid   :@MXJOBN  +
  !2.+Hex ASID :@MASD+

)INIT
 &ZWINTTL = 'MXI - ISPF Screen Images'
 .ZVARS = '(MXASL)'
 &MXASL = '1'
 .HELP = MXIHPEEK
)PROC
 VER (&MXASL,LIST,1,2)
 IF (&MXASL EQ '1')
    VER (&MXJOBN,NB)
 IF (&MXASL EQ '2')
    VER (&MASD,NB)
)END
