//SPLIT    EXEC PGM=IEBCOPY
//SYSPRINT   DD SYSOUT=*
//INSTLIB    DD DISP=SHR,DSN=SYS2.MXI.INSTLIB
//MXIPANEL   DD DISP=(NEW,CATLG),DSN=SYS2.MXI.PANELS,
//           UNIT=SYSALLDA,VOL=SER=MXIVOL,
//           LRECL=80,BLKSIZE=27920,
//           SPACE=(TRK,(30,10,40))
//MXITABLE   DD DISP=(NEW,CATLG),DSN=SYS2.MXI.TABLES,
//           UNIT=SYSALLDA,VOL=SER=MXIVOL,
//           LRECL=80,BLKSIZE=27920,
//           SPACE=(TRK,(1,1,5))
//SYSIN      DD *
